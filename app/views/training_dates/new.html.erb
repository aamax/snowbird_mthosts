<%= form_for @training_date, url: {action: "create", method: :post}, html: {class: "nifty_form"} do |f| %>
  <%= render "form", f: f %>

  <%= f.submit "Save", id: 'training_date_form_submit', class: 'btn btn-success' %>
  <%= link_to "Cancel", training_dates_path, class: "btn btn-warning" %>
<% end %>