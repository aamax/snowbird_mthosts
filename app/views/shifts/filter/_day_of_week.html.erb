<%= f.label(:dayofweek, "Day Of Week") %>
<% if params['filter'].nil? %>
    <%= f.select("dayofweek", @days, {},
                 {:class=>"span1", :multiple => true })  %>
<% else %>
    <%= f.select("dayofweek", @days, {:selected => params['filter']['dayofweek'].reject {|e| e.empty?}},
                 {:class=>"span1", :multiple => true})  %>
<% end %>
