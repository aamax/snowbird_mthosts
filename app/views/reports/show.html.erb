<% if @report == 'confirmations' %>
    <%= render 'confirmations' %>
<% elsif @report == 'phone_list' %>
    <%= render 'phone_list' %>
<% elsif @report == 'shifts_by_host' %>
    <%= render 'shifts_by_host' %>
<% elsif @report == 'shift_summary' %>
    <%= render 'shift_summary' %>
<% elsif @report == 'shift_log_review' %>
    <%= render 'shift_log_review' %>
<% else %>

    <div class='hero-unit'>
        Sorry, the <%= @report %> Report is not available.
    </div>

<% end %>